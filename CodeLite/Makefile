.PHONY: clean All

All:
	@echo "----------Building project:[ real_pong - Debug ]----------"
	@cd "real_pong" && "$(MAKE)" -f  "real_pong.mk" && "$(MAKE)" -f  "real_pong.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ real_pong - Debug ]----------"
	@cd "real_pong" && "$(MAKE)" -f  "real_pong.mk" clean
