.PHONY: clean All

All:
	@echo "----------Building project:[ echo_7_1 - Debug ]----------"
	@cd "echo_7_1" && "$(MAKE)" -f  "echo_7_1.mk" && "$(MAKE)" -f  "echo_7_1.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ echo_7_1 - Debug ]----------"
	@cd "echo_7_1" && "$(MAKE)" -f  "echo_7_1.mk" clean
