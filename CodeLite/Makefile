.PHONY: clean All

All:
	@echo "----------Building project:[ keyboard - Debug ]----------"
	@cd "keyboard" && "$(MAKE)" -f  "keyboard.mk" && "$(MAKE)" -f  "keyboard.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ keyboard - Debug ]----------"
	@cd "keyboard" && "$(MAKE)" -f  "keyboard.mk" clean
